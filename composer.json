{
	"name": "zelenin/yii2-rbac-module",
	"description": "Yii2 RBAC module with generating assignments to DB from RBAC data store file rbac.php",
	"version": "0.1.0",
	"type": "yii2-extension",
	"keywords": ["yii2", "rbac"],
	"homepage": "https://github.com/zelenin/yii2-rbac-module",
	"time": "2014-06-30",
	"license": "MIT",
	"authors": [
		{
			"name": "Aleksandr Zelenin",
			"email": "aleksandr@zelenin.me",
			"homepage": "http://zelenin.me",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/zelenin/yii2-rbac-module/issues",
		"source": "https://github.com/zelenin/yii2-rbac-module"
	},
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "*"
	},
	"autoload": {
		"psr-4": {
			"Zelenin\\yii\\modules\\Rbac\\": ""
		}
	},
    "extra": {
        "bootstrap": "Zelenin\\yii\\modules\\Rbac\\Bootstrap"
    },
	"minimum-stability": "stable"
}
